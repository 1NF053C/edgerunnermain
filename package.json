{
  "name": "edgerunner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run-once-after-npm-install": "npx ts-patch install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --runInBand --detectOpenHandles --silent=false",
    "db:migrate": "rm -rf prisma/migrations && npx prisma migrate reset -f && npx prisma generate && npx prisma migrate dev --name init",
    "db:seed": "node --import tsx prisma/seeds/seed.ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@prisma/client": "^5.16.1",
    "axios": "^1.7.2",
    "core-js": "^3.37.1",
    "dotenv": "^16.4.5",
    "mapbox-gl": "^3.5.1",
    "next": "14.2.4",
    "prisma": "^5.16.1",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.1.0",
    "ts-patch": "^3.2.1",
    "ts-runtime-checks": "^0.6.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/google.maps": "^3.55.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5"
  }
}
